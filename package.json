        "@types/supertest": "^6.0.2",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-validator": "^7.2.0",
        "firebase": "^10.13.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
