{
    "name": "t",
    "version": "1.0.0",
    "description": "SQL Sequelize Node.js JWT blogging app",
    "main": "src/server.ts",
    "scripts": {
        "start": "cross-env NODE_ENV=production nodemon",
        "dev": "nodemon",
        "build": "tsc",
        "test": "jest --config jest.config.ts --coverage",
        "initTest": "cross-env NODE_ENV=test sequelize db:create && cross-env NODE_ENV=test sequelize db:migrate && npm run test",
        "setDatabase": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all && npm run dev",
        "setProductionDatabase": "cross-env NODE_ENV=production sequelize db:seed:all"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "keywords": [
        "#jwt",
        "#sequelize",
        "#javascript",
        "#typescript",
        "#nodejs",
        "#sql",
        "#mvc"
    ],
    "author": "TAP SDBE Team G2",
    "license": "MIT",
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.14.12",
        "@types/uuid": "^10.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "sequelize-cli": "^6.6.2",
        "ts-jest": "^29.2.3",
